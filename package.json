        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.1",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.23.0",
        "@typescript-eslint/parser": "^8.23.0",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "globals": "^15.14.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.4",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.4.2",
        "rimraf": "^6.0.1",

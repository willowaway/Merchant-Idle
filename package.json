    "scripts": {
        "clean": "rimraf ./dist/src",
        "build": "npm-run-all check test clean && tsc",
        "start": "node ./dist/src",
        "dev": "tsx ./src",
        "dev:watch": "nodemon -e ts,json --exec \"npm run dev\"",
